echo "hello world" > newfile.txt
cat newfile.txt
echo "hi" > newfile.txt
cat newfile.txt
echo "hello world" > newfile.txt
cat newfile.txt
wc < newfile.txt
echo "dolor sit amet" >> newfile.txt; cat newfile.txt
wc < newfile.txt > a.txt
cat a.txt
echo "Lorem Ipsum" | wc
cat a.txt
cat newfile.txt
cat < newfile.txt | wc | cat > b.txt
cat b.txt
